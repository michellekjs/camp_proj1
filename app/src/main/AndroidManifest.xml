        <activity android:name=".TextEditActivity" />
        <activity android:name=".TextReviseActivity"></activity>
        <meta-data
            android:name="com.kakao.sdk.AppKey"
            android:value="407391d6761d133f32dba4c7d5843d99" />

    </application>



</manifest>