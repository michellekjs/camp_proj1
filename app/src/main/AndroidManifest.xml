<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.camp_proj1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:usesCleartextTraffic="true"
        android:theme="@style/Theme.Camp_proj1">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.SEARCH"/>

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>

            <meta-data
                android:name="android.app.searchable"
                android:resource="@xml/searchable"/>
        </activity>

        <activity
            android:name=".RecyclerViewClickActivity"

            >
            
        </activity>

        <activity
            android:name=".GridViewClickActivity"

            />

        <activity

            android:name=".Calender"
            android:parentActivityName=".MainActivity"

            >

        </activity>

        <activity android:name=".AddNewUserInfo">

        </activity>

        <activity android:name=".TextEditActivity"/>

    </application>
    <uses-permission android:name="android.permission.CALL_PHONE"/>
</manifest>